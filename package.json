{
	"name": "xdai-punks",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"copy": "node scripts/copy.js",
		"analyze": "source-map-explorer build/static/js/main.*",
		"start": "npm run scss && npm run translations && cross-env NODE_ENV=development node ./scripts/start.js",
		"build": "npm run scss && npm run translations && cross-env NODE_ENV=production GENERATE_SOURCEMAP=true node ./scripts/build.js",
		"build-production": "npm run scss && npm run translations && cross-env NODE_ENV=production GENERATE_SOURCEMAP=false node ./scripts/build.js && npm run sw-production && npm run copy",
		"build-development": "npm run scss && npm run translations && cross-env NODE_ENV=development GENERATE_SOURCEMAP=true node ./scripts/build.js && npm run sw-development && npm run copy",
		"build-production-map": "npm run scss && npm run translations && cross-env NODE_ENV=production GENERATE_SOURCEMAP=true node ./scripts/build.js && npm run sw-production && npm run copy",
		"test": "node scripts/test.js",
		"scss": "node ./scripts/scss.js",
		"watch-scss": "npm run scss && node-sass-chokidar src/ -o src/ --watch --recursive",
		"translations": "node ./scripts/translations.js",
		"sw-production": "cross-env SW_BASE=https://xdaipunks.com node ./scripts/updateServiceWorker.js",
		"sw-development": "cross-env SW_BASE=http://localhost:9999 node ./scripts/updateServiceWorker.js",
		"test-coverage": "node ./scripts/test.js --env=jsdom --coverage && http-server ./coverage/lcov-report -o"
	},
	"dependencies": {
		"@walletconnect/client": "^1.6.5",
		"@walletconnect/web3-provider": "^1.6.5",
		"axios": "^0.21.1",
		"bignumber.js": "^9.0.1",
		"history": "^4.10.1",
		"install": "^0.13.0",
		"invariant": "^2.2.4",
		"prop-types": "^15.7.2",
		"querystring": "^0.2.0",
		"react-infinite-scroll-component": "^6.1.0",
		"scroll-polyfill": "^0.1.1",
		"warning": "^4.0.3",
		"web3": "^1.5.2",
		"web3-eth-abi": "^1.5.2",
		"web3-utils": "^1.5.2"
	},
	"devDependencies": {
		"@babel/core": "7.12.3",
		"@pmmmwh/react-refresh-webpack-plugin": "0.4.2",
		"@svgr/webpack": "5.4.0",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@typescript-eslint/eslint-plugin": "^4.5.0",
		"@typescript-eslint/parser": "^4.5.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.0",
		"babel-loader": "8.1.0",
		"babel-plugin-named-asset-import": "^0.3.7",
		"babel-preset-react-app": "^10.0.0",
		"bfj": "^7.0.2",
		"camelcase": "^6.1.0",
		"case-sensitive-paths-webpack-plugin": "2.3.0",
		"copy-webpack-plugin": "^5.1.2",
		"css-loader": "4.3.0",
		"csv-parse": "^0.0.6",
		"dotenv": "8.2.0",
		"dotenv-expand": "5.1.0",
		"eslint": "^7.11.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-testing-library": "^3.9.2",
		"eslint-webpack-plugin": "^2.1.0",
		"file-loader": "6.1.1",
		"fs-extra": "^9.1.0",
		"html-webpack-plugin": "4.5.0",
		"identity-obj-proxy": "3.0.0",
		"jest": "26.6.0",
		"jest-circus": "26.6.0",
		"jest-resolve": "26.6.0",
		"jest-watch-typeahead": "0.6.1",
		"mini-css-extract-plugin": "0.11.3",
		"optimize-css-assets-webpack-plugin": "5.0.4",
		"pnp-webpack-plugin": "1.6.4",
		"postcss-flexbugs-fixes": "4.2.1",
		"postcss-loader": "3.0.0",
		"postcss-normalize": "8.0.1",
		"postcss-preset-env": "6.7.0",
		"postcss-safe-parser": "5.0.2",
		"prettier": "^2.1.2",
		"react": "^17.0.1",
		"react-app-polyfill": "^2.0.0",
		"react-dev-utils": "^11.0.0",
		"react-dom": "^17.0.1",
		"react-refresh": "^0.8.3",
		"resolve": "1.18.1",
		"resolve-url-loader": "^3.1.2",
		"sass-loader": "8.0.2",
		"semver": "7.3.2",
		"source-map-explorer": "^2.5.0",
		"style-loader": "1.3.0",
		"terser-webpack-plugin": "4.2.3",
		"ts-pnp": "1.2.0",
		"url-loader": "4.1.1",
		"web-vitals": "^0.2.4",
		"webpack": "4.44.2",
		"webpack-dev-server": "3.11.0",
		"webpack-manifest-plugin": "2.2.0",
		"workbox-webpack-plugin": "5.1.4"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			"last 8 versions",
			"not ie < 10"
		],
		"development": [
			"last 8 versions",
			"not ie < 10"
		]
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testRunner": "/Users/Mac/Projects/Nodejs/Muevo/my-app/node_modules/jest-circus/runner.js",
		"transform": {
			"^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"modulePaths": [],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		],
		"resetMocks": true
	},
	"babel": {
		"presets": [
			[
				"react-app",
				{
					"runtime": "automatic"
				}
			]
		]
	}
}
