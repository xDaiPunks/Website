{
	"home": {
		"path": "/",
		"type": "public",
		"redirect": "none",
		"animationType": "underlay",
		"component": "app/com/views/home/Home"
	},

	"punk": {
		"path": "/punk/:id",
		"type": "public",
		"redirect": "none",
		"animationType": "overlay",
		"component": "app/com/views/punk/Punk"
	},

	"marketPlace": {
		"path": "/market-place",
		"type": "public",
		"redirect": "none",
		"animationType": "overlay",
		"component": "app/com/views/marketPlace/MarketPlace"
	},

	"punkAccount": {
		"path": "/my-account",
		"type": "private",
		"redirect": "none",
		"animationType": "overlay",
		"component": "app/com/views/punkAccount/PunkAccount"
	},

	"notfound": {
		"type": "notfound",
		"useRedirect": true,
		"useAuthentication": true,
		"animationType": "underlay",
		"foundRedirect": "/",
		"foundAuthRedirect": "/"
	}
}
